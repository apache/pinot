{
  "tableName": "githubEvents",
  "tableType": "OFFLINE",
  "segmentsConfig": {
    "timeColumnName": "created_at_timestamp",
    "retentionTimeUnit": "DAYS",
    "retentionTimeValue": "365",
    "segmentPushType": "APPEND",
    "segmentAssignmentStrategy": "BalanceNumSegmentAssignmentStrategy",
    "schemaName": "githubEvents",
    "replication": "1"
  },
  "tenants": {
  },
  "ingestionConfig": {
    "transformConfigs": [
      {
        "columnName": "created_at_timestamp",
        "transformFunction": "fromDateTime(created_at, 'yyyy-MM-dd''T''HH:mm:ssZ')"
      }
    ]
  },
  "tableIndexConfig": {
    "loadMode": "MMAP",
    "invertedIndexColumns": [
      "id"
    ]
  },
  "task": {
    "taskTypeConfigsMap": {
      "MergeRollupTask": {
        "1d.bucketTimePeriod": "1d",
        "1d.bufferTimePeriod": "1d",
        "schedule": "0 * * * * ?"
      }
    }
  },
  "metadata": {
    "customConfigs": {
    }
  }
}
