      },
      {
        "description": "select string array literal",
        "sql": "EXPLAIN PLAN FOR SELECT ARRAY['a','b','c']",
        "output": [
          "Execution Plan",
          "\nLogicalProject(EXPR$0=[ARRAY(_UTF-8'a', _UTF-8'b', _UTF-8'c')])",
