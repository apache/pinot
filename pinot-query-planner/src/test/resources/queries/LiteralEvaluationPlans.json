        "output": [
          "Execution Plan",
          "\nLogicalProject(EXPR$0=[CAST(_UTF-8'NTH':VARCHAR CHARACTER SET \"UTF-8\"):VARCHAR CHARACTER SET \"UTF-8\"])",
          "\n  LogicalTableScan(table=[[default, a]])",
          "\n"
        ]
      },
